{
  "name": "d3-boilerplate",
  "version": "0.0.1",
  "description": "A chat example to showcase how to use `socket.io` with a static `express` server with `async` for control flow.",
  "main": "server.js",
  "author": "Mostafa Eweda <mo.eweda@gmail.com>",
  "repository": "git@github.com:Benjmhart/d3-boilerplate.git",
  "dependencies": {
    "async": "~0.2.8",
    "d3": "^4.13.0",
    "express": "~3.2.4",
    "socket.io": "~0.9.14"
  },
  "scripts": {
    "start": "webpack-dev-server --port  8080 --host 0.0.0.0 --content-base --inline --hot",
    "build": "babel src -d build",
    "lint": "eslint src --fix",
    "pretty": "prettier --use-tabs --write \"./src/*.js\"",
    "autofix": "npm-run-all -s pretty lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.11.1"
  },
  "license": "MIT"
}
